<!-- RESPONSE -->

<xsd:element name="invoiceResponse" type="inv:invoiceResponseType">
	<xsd:annotation>
		<xsd:documentation>Odpovìï na import faktury. Obsahuje podrobnosti zpracování, id a èíslo vytvoøeného dokladu.</xsd:documentation>
	</xsd:annotation>
</xsd:element>


<xsd:complexType name="invoiceResponseType">
	<xsd:complexContent>
		<xsd:extension  base="rdc:documentResponseType"/>
	</xsd:complexContent>
</xsd:complexType>

</xsd:schema>
