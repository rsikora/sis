<?xml version="1.0" encoding="Windows-1250"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			xmlns:typ="http://www.stormware.cz/schema/type.xsd"
			xmlns:rdc="http://www.stormware.cz/schema/documentresponse.xsd"
			xmlns:stk="http://www.stormware.cz/schema/stock.xsd"
			xmlns:lst="http://www.stormware.cz/schema/list.xsd"
			xmlns="http://www.stormware.cz/schema/stock.xsd"
			targetNamespace="http://www.stormware.cz/schema/stock.xsd"
			elementFormDefault="qualified"
>

<xsd:import namespace="http://www.stormware.cz/schema/type.xsd" 					schemaLocation="type.xsd"/>
<xsd:import namespace="http://www.stormware.cz/schema/list.xsd" 					schemaLocation="list.xsd"/>
<xsd:import namespace="http://www.stormware.cz/schema/documentresponse.xsd" 	schemaLocation="documentresponse.xsd"/>

<!-- **************************************************************************
		root element must be <stock>
	 ************************************************************************** -->

<xsd:element name="stock" type="stockType"/>

<!-- **************************************************************************
		stock
	 ************************************************************************** -->

<xsd:complexType name="stockType">
	<xsd:sequence>
		<xsd:element name="stockHeader" type="stk:stockHeaderType"/>
		<xsd:element name="stockDetail" type="stk:stockDetailType" minOccurs="0">
  		<xsd:annotation>
  	   	<xsd:documentation>Položky zásob u typù karta, služba, textová nebo výrobek (jen pro export).</xsd:documentation>
  	  </xsd:annotation>
  	</xsd:element>
		<xsd:element name="stockAttach" type="stk:stockAttachType" minOccurs="0">
		  <xsd:annotation>
  	   	<xsd:documentation>Svázané zásoby. Jako svázanou zásobu nelze vložit skladovou zásobu typu Komplet a Souprava (jen pro export).</xsd:documentation>
  	  </xsd:annotation>
  	</xsd:element>
		<xsd:element name="stockSerialNumber" type="stk:stockSerialNumberType" minOccurs="0">
		  <xsd:annotation>
  	   	<xsd:documentation>Výrobní èísla nebo šarže zásoby (jen pro export).</xsd:documentation>
  	  </xsd:annotation>
  	</xsd:element>  
		<xsd:element name="stockPriceItem" type="stk:stockPriceItemType" minOccurs="0">
		  <xsd:annotation>
  	   	<xsd:documentation>Slevy.</xsd:documentation>
  	  </xsd:annotation>
  	</xsd:element>
	</xsd:sequence>
	<xsd:attribute name="version" type="stk:stkVersionType" use="required"/>
</xsd:complexType>

<xsd:simpleType name="stkVersionType">
	<xsd:annotation>
		<xsd:documentation>Verze, které jsou dostupné k tomuto dokladu. Podrobnosti k jednotlivým verzím najdete na www.stormware.cz/xml</xsd:documentation>
	</xsd:annotation>
	<xsd:restriction base="xsd:string">
		<xsd:enumeration value="1.0"/>
		<xsd:enumeration value="1.1"/>
		<xsd:enumeration value="1.2"/>
		<xsd:enumeration value="1.3"/>
		<xsd:enumeration value="1.4"/>
		<xsd:enumeration value="1.5"/>
	</xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="stockHeaderType">
	<xsd:sequence>
		<xsd:element name="id" type="typ:idType" minOccurs="0">
