			</xsd:annotation>
		</xsd:element>
		<xsd:element name="orderQuantity" type="xsd:float" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Požadované množství k objednání pro použití ve funkci Automatické objednávky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
		<xsd:element name="countIssuedOrders" type="xsd:float" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Objednáno (jen pro export).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		
		<!-- záložka Doplòkové údaje -->
		<xsd:element name="shortName" type="typ:string24" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Zkrácený název skladové položky - napø. pøi tisku na registraèní pokladnì.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="typeRP" type="typ:refType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Pøíznak pro registraèní pokladnu.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="guaranteeType" type="typ:guaranteeTypeType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Typ záruky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="guarantee" type="xsd:integer" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Délka záruky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="producer" type="typ:string90" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Výrobce skladové položky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="yield" type="lst:accountType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Výnos. Úèet pro úètování výnosù z prodeje zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
		<xsd:element name="classOfStock" type="stk:classOfStockType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Úètování metodou A - Druh zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>	
		<xsd:element name="acc" type="lst:accountType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Úètování metodou A - analitický úèet zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		
		<!-- Intrastat -->
		<!-- ToDo -->
		
		<!-- Cizí názvy zásoby -->
		<xsd:element name="foreignName1" type="typ:string90" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Cizí název skladové položky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
		<xsd:element name="foreignNameComplement1" type="typ:string90" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Doplòující text èi poznámka k názvu skladové zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="foreignName2" type="typ:string90" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Cizí název skladové položky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
		<xsd:element name="foreignNameComplement2" type="typ:string90" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Doplòující text èi poznámka k názvu skladové zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
				
		<!-- záložka Internet -->
		<xsd:element name="description" type="typ:string240" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Struèný popis.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="description2" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Podrobný popis.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="picture" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Obrázek skladové položky.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="200"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
		
		<xsd:element name="categories" type="stk:categoriesType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Podrobný popis.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="relatedStocks" type="stk:relatedStocksType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Seznam ID souvisejícíh zásob.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="alternativeStocks" type="stk:alternativeStocksType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Aternativních zásob (description alternative stocks).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="intParameters" type="stk:intParametersType" minOccurs="0" maxOccurs="1">
			<xsd:annotation>
				<xsd:documentation>Internetového parametru(description parameters).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>

		<!-- Poznámka -->
		<xsd:element name="note" type="xsd:string" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Poznámka.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="parameter" type="typ:parameterDocType" minOccurs="0" maxOccurs="64">
			<xsd:annotation>
				<xsd:documentation>Volitelný parametr.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
	
</xsd:complexType>

<!-- popis Kategorie(description categories) -->
<xsd:complexType name="categoriesType">
	<xsd:sequence>
		<xsd:element name="idCategory" type="typ:idType" minOccurs="0" maxOccurs="unbounded" />
	</xsd:sequence>
</xsd:complexType>

<!-- popis Souvisejících zásoby(description Related stock) -->
<xsd:complexType name="relatedStocksType">
	<xsd:annotation>
		<xsd:documentation>Seznam ID souvisejícíh zásob.</xsd:documentation>
	</xsd:annotation>
	<xsd:sequence>
		<xsd:element name="idStocks" type="typ:stockItemType" minOccurs="0" maxOccurs="unbounded" />
	</xsd:sequence>
</xsd:complexType>

<!-- popis Aternativních zásob (description alternative stocks) -->
<xsd:complexType name="alternativeStocksType">
	<xsd:annotation>
		<xsd:documentation>Seznam ID alternativních zásob.</xsd:documentation>
	</xsd:annotation>
	<xsd:sequence>
		<xsd:element name="idStocks" type="typ:stockItemType" minOccurs="0" maxOccurs="unbounded" />
	</xsd:sequence>
</xsd:complexType>

<!-- popis Internetového parametru(description parameters) -->
<xsd:complexType name="intParametersType">
	<xsd:sequence>
		<xsd:element name="intParameter" type="stk:intParameterType" minOccurs="0" maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="intParameterType">
	<xsd:sequence>
		<xsd:element name="parameterID" type="typ:idType" minOccurs="0" maxOccurs="1"/>
		<xsd:element name="parameterValue" type="typ:string255" minOccurs="0" maxOccurs="1"/>
	</xsd:sequence>
</xsd:complexType>

<!-- položky skladové zásoby -->
<xsd:complexType name="stockDetailType">
	<xsd:sequence>
		<xsd:element name="stockItem" type="stk:stockItemType" maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="stockItemType">
	<xsd:sequence>
		<xsd:group ref="stk:myGroupStockItem"/>
		<xsd:element name="stockPriceItem" type="stk:stockPriceItemType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Úprava cen položky zásob u jednotlivých cenových hladin. Jen u typu Komplet.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

<!-- svázané zásoby -->
<xsd:complexType name="stockAttachType">
	<xsd:sequence>
		<xsd:element name="attachItem" type="stk:attachItemType" maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="attachItemType">
	<xsd:sequence>
		<xsd:group ref="stk:myGroupStockItem"/>
		<xsd:element name="warehouse" type="typ:boolean" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Naskladnit. Svázaná zásoba vloží do dokladu i pøi naskladnìní skladové karty.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="notExpedite" type="typ:boolean" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Nevyskladòovat. Svázaná zásoba se nevloží do dokladu pøi vyskladnìní skladové karty.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="edit" type="typ:boolean" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Úprava. Pokud volba Úprava nebude na záložce Svázané zatržena, budou se ceny automaticky aktulizovat dle provedených zmìn na skladové kartì.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="stockPriceItem" type="stk:stockPriceItemType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Úprava cen svázané zásoby u jednotlivých cenových hladin.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>	
</xsd:complexType>

<xsd:group name="myGroupStockItem">
	<xsd:sequence>
	  <xsd:element name="id" type="typ:idType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>ID skladové zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	  <xsd:element name="storage" type="typ:refTypeStorage">
			<xsd:annotation>
				<xsd:documentation>Èlenìní.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
    <xsd:element name="code" type="typ:stockIdsType">
			<xsd:annotation>
				<xsd:documentation>Øetìzcový identifikátor skladové položky.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="name" type="typ:string90">
			<xsd:annotation>
				<xsd:documentation>Název skladové položky (jen pro export).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
		<xsd:element name="count" type="xsd:float" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Stav zásoby (jen pro export).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="quantity" type="xsd:float" minOccurs="0">
  		<xsd:annotation>
  			<xsd:documentation>Množství.</xsd:documentation>
  		</xsd:annotation>
  	</xsd:element>		
    <xsd:element name="unit" type="typ:unitType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Mìrná jednotka (jen pro export).</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
	</xsd:sequence>
</xsd:group>

<xsd:complexType name="stockSerialNumberType">
	<xsd:sequence>
		<xsd:element name="serialNumberItem" type="stk:serialNumberItemType" maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="serialNumberItemType">
	<xsd:sequence>
		<xsd:element name="id" type="typ:idType" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>ID výrobní èíslo nebo šarže.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="serialNumber" type="typ:serialNumberType">
			<xsd:annotation>
				<xsd:documentation>Výrobní èíslo nebo šarže.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="count" type="xsd:float" minOccurs="0">
			<xsd:annotation>
				<xsd:documentation>Stav zásoby.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="expiration" type="xsd:date" minOccurs="0">
			<xsd:annotation>
